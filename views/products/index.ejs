<h1>All Products</h1>
<table id="list">
    <thead>
        <tr>
            <th>Product</th>
            <th>Price</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% products.forEach(function(p) { %>
            <tr>
                <td><%= p.name %></td>
                <td><%= p.price %></td>
                <td><a href="/products/<%= p._id %>">DETAILS</a></td>
            </tr>
        <% }); %>
    </tbody>
</table>
